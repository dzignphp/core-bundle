{
    "name": "dzignphp/core-bundle",
    "type": "symfony-bundle",
    "description": "dzign",
    "keywords": [
        "Admin"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jérôme Drzewinski",
            "email": "jdrzewinski@dzign.fr"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "ext-iconv": "*",
        "doctrine/common": "^2.8",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/orm": "^2.5.11",
        "doctrine/persistence": "^1.3.4",
        "symfony/asset": "^5.0",
        "symfony/cache": "^5.0",
        "symfony/config": "^5.0",
        "symfony/dependency-injection": "^5.0",
        "symfony/doctrine-bridge": "^5.0",
        "symfony/dotenv": "^5.0",
        "symfony/event-dispatcher": "^5.0",
        "symfony/expression-language": "^5.0",
        "symfony/finder": "^5.0",
        "symfony/flex": "^1.6",
        "symfony/form": "^5.0",
        "symfony/framework-bundle": "^5.0",
        "symfony/http-foundation": "^5.0",
        "symfony/http-kernel": "^5.0",
        "symfony/polyfill-mbstring": "^1.7",
        "symfony/property-access": "^5.0",
        "symfony/security-bundle": "^5.0",
        "symfony/translation": "^5.0",
        "symfony/twig-bridge": "^5.0",
        "symfony/twig-bundle": "^5.0",
        "symfony/validator": "^5.0",
        "symfony/yaml": "^5.0",
        "twig/twig": "^3.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Dzign\\Core\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Dzign\\Core\\Tests\\": "/Tests/"
        }
    }
}
